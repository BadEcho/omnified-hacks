function ActivateLoggers()
  if loggersTimer == nil then
    loggersTimer = createTimer(getMainForm())
  end

  loggersTimer.Interval = 200
  loggersTimer.OnTimer = function()
    local file = assert(io.open("stats.txt", "w"))

    local omniBulletCount = readInteger("bulletCount")

    if omniBulletCount ~= nil then
      file:write("Omni Shots Fired: ", omniBulletCount, "\n")
    end

    local omniDistance = readInteger("playerFitbit")
    local inkyDistance = readInteger("secondPlayerFitbit")

    if omniDistance ~= nil then
      file:write("Omni Fitbit: ", math.floor(omniDistance * 0.04), "\n")
    end

    if inkyDistance ~= nil then
      file:write("Inky Fitbit: ", math.floor(inkyDistance * 0.04), "\n")
    end

    file:close()
  end

  loggersTimer.Enabled = true
end

