//
// getPlayer: Grabs the player structures.
//            Health: x+A0
//
//            rax == 1: health
//            rax == 2: shield   only player when rbx has value
//            rax == A: unknown
//            rax == B: unknown
//            rax == C: unknown (sniper max located at F0)
//            rax == E: unknown
//            rax == D: unknown
//            rax == F: unkown
//            rax == 10: unknwon
//            rax == 11: unknown
//            rax == 12: unkonwn
//0000000286CC5678  R13==0x1 and RAX==0x1 and RBP==0x2
getPlayer:
  cmp rbx,0
  je getPlayerOriginalCode
  cmp r13,1
  je getPlayerOrTeammate
  cmp r13,38
  jne getPlayerOriginalCode
  cmp rax,1
  je getPlayerHealth
  cmp rax,2
  je getPlayerShield
  jmp getPlayerOriginalCode
getPlayerOrTeammate:
  cmp rbp,1c
  jne getTeammate
  cmp rax,b
  je getPetRespawn
  jmp getPlayerOriginalCode
getPetRespawn:
  push rbx
  mov rbx,pPetRespawn
  mov [rbx],rcx
  pop rbx
  jmp getPlayerOriginalCode
getTeammate:
  cmp rbp,2
  je getSecondPlayer
  jmp getPlayerOriginalCode
getSecondPlayer:
  cmp rax,1
  je getSecondPlayerHealth
  cmp rax,2
  je getSecondPlayerShield
  jmp getPlayerOriginalCode
getSecondPlayerHealth:
  push rbx
  push rdx
  mov rbx,[rcx+8]
  mov rdx,pPlayerHealthType
  cmp rbx,[rdx]
  pop rdx
  pop rbx
  jne getPlayerOriginalCode
  push rbx
  mov rbx,pSecondPlayerHealth
  mov [rbx],rcx
  pop rbx
  jmp getPlayerOriginalCode
getSecondPlayerShield:
  push rbx
  mov rbx,pSecondPlayerShield
  mov [rbx],rcx
  pop rbx
  jmp getPlayerOriginalCode
getPlayerHealth:
  push rbx
  push rcx
  push rdx
  mov rbx,pPlayerHealth
  mov [rbx],rcx
  mov rbx,[rcx+8]
  mov rdx,pPlayerHealthType
  mov [rdx],rbx
  pop rdx
  pop rcx
  pop rbx
  push rbx
  push rcx
  mov rbx,pPlayerFfyl
  add rcx,C98
  mov [rbx],rcx
  pop rcx
  pop rbx
  jmp getPlayerOriginalCode
getPlayerShield:
  push rbx
  mov rbx,pPlayerShield
  mov [rbx],rcx
  pop rbx
  jmp getPlayerOriginalCode
getPlayerOriginalCode:
  mov eax,[rcx]
  cmp [rdx+10],eax
  jmp getPlayerReturn

omniPlayerHook:
  jmp getPlayer
getPlayerReturn:

