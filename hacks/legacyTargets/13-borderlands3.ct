//
// fuckSpeedUp: Boosts the movement of enemies and (optionally) the player.
//
//              Updated coords: xmm12
//              Original coords: [rdi+220]
fuckSpeedUp:
  pushf
  cmp r14,1
  jne applyChange
  cmp [pCoords],0
  je applyChange
  sub rsp,16
  movdqu [rsp],xmm13
  sub rsp,16
  movdqu [rsp],xmm14
  sub rsp,16
  movdqu [esp],xmm15
  movdqu xmm13,[rsi]
  cmp rdi,[pCoords]
  je updateFirstPlayerFitbit
  cmp rdi,[pSecondCoords]
  je updateSecondPlayerFitbit
  cmp rdi,[pPetCoords]
  je applyPlayerMultiplier
applyEnemyMultiplier:
  sub rsp,16
  movdqu [rsp],xmm3
  movss xmm3,[beautyWidth]
  movss [rdi+208],xmm3
  movss xmm3,[beautyHeight]
  movss [rdi+20C],xmm3
  movdqu xmm3,[rsp]
  add rsp,16
  movss xmm14,[enemySpeedMultiplier]
  jmp updateChange
updateFirstPlayerFitbit:
  inc [playerFitbit]
  jmp applyPlayerMultiplier
updateSecondPlayerFitbit:
